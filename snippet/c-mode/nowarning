# -*- mode: snippet -*-
# name: nowarning
# key: nowarning
# --
#if (${1:$$(upcase yas-text)})
${1:$(let ((cc (upcase yas-text)))
         (cond ((string= "MSVC" cc)
				        "#  pragma warning(disable:XXX)")
				       ((string= "GCC" cc)
							 "#  pragma GCC diagnostic ignored \\"XXX\\"")
							 ((string= "CLANG" cc)
							 "#  pragma clang diagnostic ignored \\"XXX\\"")))}$0
#endif /* end of $1 */
       

