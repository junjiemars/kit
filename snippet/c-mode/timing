# -*- mode: snippet -*-
# name: timing
# key: timeing
# --
#include <time.h>
#define _timing_(E, W)                          \
do                                              \
 {                                              \
   clock_t _epoch_1_ = clock();                 \
   (E);                                         \
   W = (clock() - _epoch_1_);                   \
 } while (0)
$0
